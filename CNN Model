 is not None:\n","                X.append(mel[..., np.newaxis])  # (128, time, 1)\n","                y.append(label)\n","\n","        return np.array(X), np.array(y)\n","\n","    def on_epoch_end(self):\n","        if self.shuffle:\n","            zipped = list(zip(self.file_paths, self.labels))\n","            random.shuffle(zipped)\n","            self.file_paths, self.labels = zip(*zipped)\n"],"metadata":{"id":"uavDtFfv4DtU","executionInfo":{"status":"ok","timestamp":1743689498211,"user_tz":-330,"elapsed":6,"user":{"displayName":"Tharindu Munasinghe","userId":"05529032449695582270"}}},"execution_count":9,"outputs":[]},{"cell_type":"code","source":["file_paths = []\n","labels = []\n","\n","for label in os.listdir(DATA_PATH):\n","    folder = os.path.join(DATA_PATH, label)\n","    if os.path.isdir(folder):\n","        for file in os.listdir(folder):\n","            if file.endswith('.wav'):\n","                file_paths.append(os.path.join(folder, file))\n","                labels.append(label)\n","\n","le = LabelEncoder()\n","labels_encoded = le.fit_transform(labels)\n","labels_onehot = to_categorical(labels_encoded)\n","\n","train_paths, test_paths, train_labels, test_labels = train_test_split(\n","    file_paths, labels_onehot, test_size=0.2, stratify=labels_encoded, random_state=42\n",")\n"],"metadata":{"id":"uhEwD3cX4E02","executionInfo":{"status":"ok","timestamp":1743689504210,"user_tz":-330,"elapsed":5998,"user":{"displayName":"Tharindu Munasinghe","userId":"05529032449695582270"}}},"execution_count":10,"outputs":[]},{"cell_type":"code","source":["train_gen = AudioDataGenerator(train_paths, train_labels, augment=True)\n","val_gen = AudioDataGenerator(test_paths, test_labels, augment=False)\n"],"metadata":{"id":"bMxkbns44F3o","executionInfo":{"status":"ok","timestamp":1743689504224,"user_tz":-330,"elapsed":15,"user":{"displayName":"Tharindu Munasinghe","userId":"05529032449695582270"}}},"execution_count":11,"outputs":[]},{"cell_type":"code","source":["inpu